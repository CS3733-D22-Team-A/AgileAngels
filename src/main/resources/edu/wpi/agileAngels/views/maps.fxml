<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Button?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.agileAngels.MapsController">
   <children>
       <Button fx:id="homeButton" layoutX="248.0" layoutY="347.0" mnemonicParsing="false" onAction="#goHome" text="Return to Home" />
       <MenuButton fx:id="mapMenu" layoutX="71.0" layoutY="184.0" mnemonicParsing="false" text="Select Floor">
        <items>
          <MenuItem id="floorOne" fx:id="floorOne" mnemonicParsing="false" onAction="#changeMap" text="Floor 1" />
          <MenuItem id="floorTwo" fx:id="floorTwo" mnemonicParsing="false" onAction="#changeMap" text="Floor 2" />
        </items>
      </MenuButton>
      <ImageView id="floorTwoMap" fx:id="floorTwoMap" fitHeight="292.0" fitWidth="293.0" layoutX="262.0" layoutY="54.0" preserveRatio="true">
         <image>
            <Image url="@../assets/Floor2_Tower_Final.jpg" />
         </image>
      </ImageView>
      <ImageView id="floorOneMap" fx:id="floorOneMap" fitHeight="295.0" fitWidth="293.0" layoutX="262.0" layoutY="51.0" preserveRatio="true">
         <image>
            <Image url="@../assets/Floor1_Tower_Final.jpg" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
